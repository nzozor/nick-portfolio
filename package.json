{
  "name": "nick-website",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "start:ssl": "ng serve --ssl --ssl-key localhost.key  --ssl-cert localhost.crt",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:prod": "npm build:ssr",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/nick-website/server/main.js",
    "build:ssr": "ng build --prod && ng run nick-website:server:production",
    "build:client-and-server-bundles": "ng build --prod --aot && ng run nick-website:server:production --bundleDependencies all",
    "dev:ssr": "ng run nick-website:serve-ssr",
    "prerender": "ng run nick-website:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.1",
    "@angular/common": "~11.0.1",
    "@angular/compiler": "~11.0.1",
    "@angular/core": "~11.0.1",
    "@angular/forms": "~11.0.1",
    "@angular/platform-browser": "~11.0.1",
    "@angular/platform-browser-dynamic": "~11.0.1",
    "@angular/platform-server": "~11.0.1",
    "@angular/router": "~11.0.1",
    "@fortawesome/angular-fontawesome": "^0.8.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@nguniversal/express-engine": "^11.0.1",
    "angular-http-server": "^1.9.0",
    "angular-in-memory-web-api": "^0.11.0",
    "bootstrap": "^4.5.3",
    "express": "^4.15.2",
    "fs": "0.0.1-security",
    "moment": "^2.29.1",
    "ngx-useful-swiper": "^10.0.1",
    "rxjs": "~6.6.0",
    "swiper": "^6.6.2",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2",
    "xml-js": "^1.6.11",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.2",
    "@angular/cli": "~11.0.2",
    "@angular/compiler-cli": "~11.0.1",
    "@nguniversal/builders": "^11.0.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@types/uuid": "^8.3.0",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
